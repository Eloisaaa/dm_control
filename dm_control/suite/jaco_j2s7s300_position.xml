  <!- position actuators
  ~10 damping is important or the limbs shake
  kp should be around 55 for position actuators - too high and things become unstable-->
  <!- order of joint specification is important - major to minor. fingers are optional -->
  <!- min/max ctrl position of joints 1,3,5,7 is actually -+10,000 but that seemed excessive -->
  <actuator>
  <position joint='jaco_joint_1'  name='pos_act_joint_1' kp="100"  ctrlrange='-628 628' ctrllimited='true'/>
    <position joint='jaco_joint_2'  name='pos_act_joint_2' kp="170" ctrlrange='.8 5.46' ctrllimited='true'/>
    <position joint='jaco_joint_3'  name='pos_act_joint_3' kp="100" ctrlrange='-628 628' ctrllimited='true'/>
    <position joint='jaco_joint_4'  name='pos_act_joint_4' kp="170" ctrlrange='.5 5.759' ctrllimited='true'/>
    <position joint='jaco_joint_5'  name='pos_act_joint_5' kp="15"  ctrlrange='-628 628' ctrllimited='true'/>
    <position joint='jaco_joint_6'  name='pos_act_joint_6' kp="15"  ctrlrange='1.1344 5.148' ctrllimited='true'/>
    <position joint='jaco_joint_7'  name='pos_act_joint_7' kp="15"  ctrlrange='-628 628' ctrllimited='true'/>
    <position joint='jaco_joint_finger_1' kp="15" name='pos_act_finger_1'  ctrlrange="0 118.68" ctrllimited="true"/>
    <position joint='jaco_joint_finger_2' kp="15" name='pos_act_finger_2'  ctrlrange="0 118.68" ctrllimited="true"/>
    <position joint='jaco_joint_finger_3' kp="15" name='pos_act_finger_3'  ctrlrange="0 118.68" ctrllimited="true"/>
    <position joint='jaco_joint_finger_tip_1' kp="15" name='pos_act_finger_tip_1' ctrlrange="0 118.68" ctrllimited="true"/>
    <position joint='jaco_joint_finger_tip_2' kp="15" name='pos_act_finger_tip_2' ctrlrange="0 118.68" ctrllimited="true"/>
    <position joint='jaco_joint_finger_tip_3' kp="15" name='pos_act_finger_tip_3' ctrlrange="0 118.68" ctrllimited="true"/>

  </actuator>

